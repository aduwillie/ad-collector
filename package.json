{
  "name": "ad-collector",
  "version": "1.0.0",
  "description": "This is a data collection system for gathering and analysing all kinds of data.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --fix",
    "logs:deployer": "docker-compose logs -f deployer",
    "logs:webui": "docker-compose logs -f webui",
    "start": "docker-compose up -d webui",
    "start:webui": "./node_modules/.bin/babel-node src/webui/index.js",
    "start:webui:dev": "./node_modules/.bin/nodemon --mode development --exec 'npm run start:webui'",
    "test": "npm run test:webui",
    "test:watch": "npm run test:webui:watch",
    "test:webui": "NODE_ENV=test ./node_modules/.bin/jest",
    "test:webui:watch": "NODE_ENV=test ./node_modules/.bin/jest",
    "build": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aduwillie/ad-collector.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aduwillie/ad-collector/issues"
  },
  "pre-commit": [
    "lint"
  ],
  "homepage": "https://github.com/aduwillie/ad-collector#readme",
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.init.js"
    ],
    "verbose": true,
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@hapi/boom": "^7.4.9",
    "@hapi/cookie": "^10.1.2",
    "@hapi/hapi": "^18.4.0",
    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.4",
    "@hapi/vision": "^5.5.4",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.1.0",
    "hapi-alive": "^2.0.4",
    "hapi-auth-jwt2": "^8.6.2",
    "hapi-pino": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "pug": "^2.0.4",
    "schmervice": "^1.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "pre-commit": "^1.2.2",
    "pug-loader": "^2.4.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  }
}
